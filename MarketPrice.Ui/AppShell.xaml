<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MarketPrice.Ui.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:views="clr-namespace:MarketPrice.Ui.Views"
    Title="MarketPrice.Ui">

    <ShellContent
        Title="Onboarding"
        ContentTemplate="{DataTemplate views:Onboarding}"
        Route="Onboarding" />

    <ShellContent
        Title="Welcome"
        ContentTemplate="{DataTemplate views:Welcome}"
        Route="Welcome" />

    <ShellContent
        Title="Register"
        ContentTemplate="{DataTemplate views:Register}"
        Route="Register" />

    <ShellContent
        Title="Login"
        ContentTemplate="{DataTemplate views:Login}"
        Route="Login"/>

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate views:Home}"
        Route="Home"/>

</Shell>
